<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="top.icespite.fridahooker">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="false"
        android:icon="@mipmap/frida"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/frida"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="top.icespite.fridahooker.view.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="top.icespite.fridahooker.view.SettingsActivity" android:label="@string/settings" android:exported="false"/>
    </application>
</manifest>